{
    "name": "@caffeine/auth",
    "author": {
        "name": "Alan Reis",
        "email": "alanreisanjo@gmail.com",
        "url": "https://hoyasumii.dev"
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "typesVersions": {
        "*": {
            "*": [
                "./dist/*/index.d.ts"
            ]
        }
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./*": {
            "types": "./dist/*/index.d.ts",
            "import": "./dist/*/index.js",
            "require": "./dist/*/index.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "type": "module",
    "scripts": {
        "build": "tsup 'src/**/index.ts' --format cjs,esm --dts --tsconfig tsconfig.build.json --clean",
        "test:unit": "vitest run",
        "test:coverage": "vitest run --coverage",
        "setup": "bun run build && bun link",
        "prepare": "husky"
    },
    "devDependencies": {
        "@faker-js/faker": "^10.2.0",
        "@types/bun": "latest",
        "@vitest/coverage-v8": "^4.0.18",
        "tsup": "^8.5.1",
        "vite-tsconfig-paths": "^6.0.5",
        "vitest": "^4.0.18",
        "elysia": "^1.4.26",
        "@elysiajs/bearer": "^1.4.2",
        "@commitlint/cli": "^20.4.1",
        "@commitlint/config-conventional": "^20.4.1",
        "husky": "^9.1.7"
    },
    "peerDependencies": {
        "typescript": "^5",
        "elysia": "^1.4.26"
    },
    "dependencies": {
        "@caffeine/constants": "link:@caffeine/constants",
        "@caffeine/entity": "link:@caffeine/entity",
        "@caffeine/errors": "link:@caffeine/errors",
        "@caffeine/models": "link:@caffeine/models",
        "@caffeine/schema": "link:@caffeine/schema",
        "@caffeine/cache": "link:@caffeine/cache",
        "ioredis-mock": "^8.13.1",
        "jose": "^6.1.3"
    }
}
